# macOS system
/System/Library/Frameworks/Python.framework/Versions/2.7
/usr/bin/

# macOS installer from python.org
/Library/Frameworks/Python.framework/Versions/2.7
/usr/local/bin/

# Own compilations
$HOME/local/python/2.7
$HOME/local/python/2.7/Python.framework/Versions/2.7
$HOME/local/python/3.6
$HOME/local/python/3.6/Python.framework/Versions/3.6
_2.7_activetcltk/bin
_2.7_stdtcltk/bin
_3.6_activetcltk/bin
_3.6_stdtcltk/bin

# Homebrew (only for homebrew builds)
/usr/local/Cellar/python/2.7.15/
/usr/local/Cellar/python@2/2.7.15/Frameworks/
/usr/local/Cellar/python/3.6.5/
/usr/local/Cellar/python/3.6.5/Frameworks/  

# Virtualbox library
/Library/Python/2.7/site-packages
